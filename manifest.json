{
    "manifest_version": 3,
    "name": "Quizlet Downloader",
    "version": "1.2.3",
    "description": "Scraping content from Quizlet and saving it locally.",
    "homepage_url": "https://github.com/NekoZ410/Quizlet-Downloader",
    "permissions": ["activeTab", "scripting", "downloads", "storage"],
    "host_permissions": ["https://quizlet.com/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icons/icon16.png",
            "32": "images/icons/icon32.png",
            "48": "images/icons/icon64.png",
            "128": "images/icons/icon128.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["https://quizlet.com/*"],
            "js": ["content.js"]
        }
    ]
}
